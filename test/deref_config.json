            },
            {
                "prefix": "org",
                "uri": "http://www.w3.org/ns/org#"
            },
            {
                "prefix": "skos",
                "uri": "http://www.w3.org/2004/02/skos/core#"
            }
        ],
        "ASSERTION_PATHS": [
            "schema:givenName",
            "schema:familyName",
            "schema:image",
            "schema:identifier",
            "org:hasMembership/org:organization/schema:name",
            "org:hasMembership/org:role/skos:prefLabel"
        ],
        "TEMPLATE": "person",
        "MAPPING": {
            "name": "schema:givenName",
            "family": "schema:familyName",
            "image": "schema:image",
            "orcid": "schema:identifier",
            "organization": "org:hasMembership/org:organization ?a. \n ?a a org:Organization .\n ?a schema:name",
            "job_position": "org:hasMembership/org:role/skos:prefLabel"
        }
    },
